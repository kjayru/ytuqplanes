.all
	width 100%
	relative()
.limit
	width 100%
	max-width 1114px
	padding 0 15px
	margin auto
	relative()
	&.m--1600
		max-width 1600px
	&.m--1200
		max-width 1200px
.clear
	clear both
	display block
.left
	float left
.right
	float right
.table
	width 100%
	height 100%
	display table
	relative()
.table__cell
	display table-cell
	vertical-align middle
	relative()
	text-align center
.table__contenido
	display inline-block
	relative()
.swiper-container
.swiper-wrapper
.swiper-slide
	width 100%
	height 100%
	position relative
.imgfull
	width 100%
.hnos
	display inline-block
	vertical-align middle
.none
	display none
.bg-cerrar
	width 100%
	height 100%
	position absolute
	top 0
	left 0
	z-index 0

/* Plugins reset */
.datepicker--day-name
	color naranja
.datepicker--cell
	transition background ease .25s
	&.-current-
		color celeste
		&.-disabled-
			color rgba(60, 179, 189, 0.8)
	&.-disabled-.-focus-
		background none
	&.-selected-
		background celeste
		&.-focus-
			background celesteOscuro


